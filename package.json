  "main": "build/urpc.cjs.js",
  "browser": "build/urpc.umd.js",
  "type": "module",
  "exports": {
    "import": "./src/index.js",
    "require": "./build/urpc.cjs.js",
    "browser": "./build/urpc.esm.js"
  },
  "devDependencies": {
    "allow-publish-tag": "^2.1.1",
